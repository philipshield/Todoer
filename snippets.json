{
	"Create TODO item": {
		"prefix": ["todo", "create todo"],
		"body": [
			"- ${3|[ ],[x]|} ${1|🎨,🐛,📃,🔥|} ${2:description}"
		],
		"description": "Create task (grouping of todo items)"
	},
	"Create todo group": {
		"prefix": ["task", "group"],
		"body": [
			"$(#${1:task-id}) ${2:description}:"
		],
		"description": "Create TODO item"
	}
}